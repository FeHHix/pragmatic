{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015"],
    "jsx": "react",
    "baseUrl": "src",
    "outDir": "dist",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "types": ["node"],
    "stripInternal": true,
    "esModuleInterop": true,

    "paths": {
      "Modules/*": ["Modules/*"],
    },
  },
}
